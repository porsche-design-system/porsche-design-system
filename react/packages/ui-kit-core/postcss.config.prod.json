{
  "use": [
    "postcss-flexboxfixer",
    "postcss-gradientfixer",
    "autoprefixer"
  ],
  "output": "dist/porsche-stylesheets.min.css",
  "autoprefixer": {
    "browsers": [
      "Last 2 versions",
      "Android >= 4",
      "Chrome >= 53",
      "Edge >= 15",
      "Firefox >= 52",
      "IE >= 11",
      "Safari >= 9",
      "iOS >= 8",
      "Samsung >= 5"
    ]
  }
}
