{
  "presets": [
    "env",
    "react",
    "stage-1"
  ],
  "plugins": [
    "lodash",
    ["transform-react-remove-prop-types", {
      "mode": "wrap"
    }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }]
  ],
  "env": {
    "development": {
      "plugins": "react-hot-loader/babel"
    }
  }
}
