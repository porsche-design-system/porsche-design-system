{
  "name": "@porsche/plugin-node-sass-dependency-collector",
  "version": "1.0.1",
  "description": "Collects all related scss dependencies",
  "main": "index.js",
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "node-sass-tilde-importer": "^1.0.2"
  },
  "devDependencies": {
    "npm-check": "^5.9.0"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:myporsche/pouikit-porsche-ui-kit.git"
  },
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "authenticate-registry": "(npm whoami --registry=$npm_config_repository_url || npm login --registry=$npm_config_repository_url)",
    "publish-artifactory": "npm run authenticate-registry --repository_url=$npm_package_publishConfig__porsche_registry && npm publish",
    "clean": "rm -rf ./node_modules",
    "upgrade-interactive": "npm-check --update"
  },
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "contributors": [
    "Mitko Tschimev <net.mitko.tschimev@mhp.com>",
    "Stephan Schr√∂ter <stephan.schroeter@mhp.com>"
  ],
  "license": "MIT",
  "publishConfig": {
    "@porsche:registry": "https://porscheui.jfrog.io/porscheui/api/npm/npm-local/"
  }
}
