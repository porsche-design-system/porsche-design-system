{
  "name": "components-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true concurrently -k -s 'first' -n 'cdn,react' 'yarn start:cdn-server' 'react-scripts start'",
    "start:cdn-server": "start-cdn-local",
    "build": "cd projects/components-wrapper && yarn build",
    "test": "yarn test:mocks && yarn test:vrt && yarn test:cbt",
    "test:mocks": "jest --config='./tests/mocks/config/jest.config.js'",
    "test:vrt": "concurrently -k -s 'first' -n 'server,vrt' 'BROWSER=none yarn start' 'wait-on -t 240000 http-get://localhost:3000 && ts-node --project 'tests/vrt/config/tsconfig.json' node_modules/.bin/jasmine --config=\"tests/vrt/config/jasmine.config.json\"'",
    "test:cbt": "concurrently -k -s 'first' -n 'server,cbt' 'BROWSER=none yarn start' 'wait-on -t 240000 http-get://localhost:3000 && ts-node --project 'tests/cbt/config/tsconfig.json' node_modules/.bin/jasmine --config=\"tests/cbt/config/jasmine.config.json\"'",
    "clean": "rm -rf './node_modules' && rm -rf './yarn-error.log' && rm -rf './build'"
  },
  "dependencies": {
    "@porsche-design-system/components-react": "1.5.0-rc.0",
    "@porsche-design-system/utilities": "1.0.0-rc.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@porsche-design-system/cdn-local": "0.0.0",
    "@porsche-design-system/visual-regression-tester": "^2.0.1",
    "@porsche-ui/cross-browser-tester": "^1.0.2",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "concurrently": "^5.2.0",
    "jasmine": "^3.5.0",
    "puppeteer": "~3.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.1"
  },
  "resolutions": {
    "@types/react": "^16.9.17"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "SEE LICENSE IN https://designsystem.porsche.com",
  "browserslist": {
    "production": [
      "last 2 version",
      ">1%",
      "not dead",
      "not ie < 11",
      "not op_mini all"
    ],
    "development": [
      "last 2 version",
      ">1%",
      "not dead",
      "not ie < 11",
      "not op_mini all"
    ]
  }
}
