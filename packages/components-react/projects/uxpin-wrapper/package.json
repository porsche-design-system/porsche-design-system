{
  "name": "@porsche-design-system/uxpin",
  "version": "0.0.0",
  "private": true,
  "scope": "@porsche-design-system",
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "scripts": {
    "prestart": "yarn clean:build && yarn build",
    "start": "concurrently -k -s 'first' -n 'cdn,uxpin' 'serve-cdn' 'uxpin-merge --disable-tunneling'",
    "build": "cp ../components-wrapper/src/provider.tsx ./src && cp ../components-wrapper/src/hooks.ts ./src && cp ../components-wrapper/src/utils.ts ./src",
    "clean": "yarn clean:node_modules && yarn clean:build",
    "clean:node_modules": "rm -rf ./node_modules && rm -rf ./yarn-error.log",
    "clean:build": "rm -rf ./.uxpin-merge"
  },
  "dependencies": {
    "@porsche-design-system/components-js": "2.2.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.4",
    "@uxpin/merge-cli": "^2.7.6",
    "babel-loader": "^8.2.2",
    "eslint-loader": "^4.0.2",
    "typescript": "~4.2.4"
  }
}
