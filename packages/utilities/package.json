  "scripts": {
    "build": "yarn --cwd ./projects/utilities build && yarn --cwd ./projects/styles build",
    "test": "yarn test:unit",
    "test:unit": "yarn --cwd ./projects/utilities test:unit && yarn --cwd ./projects/styles test:unit",
