{
  "name": "@porsche-design-system/icons",
  "version": "0.0.0",
  "private": true,
  "scope": "@porsche-design-system",
  "main": "dist/umd/index.js",
  "module": "dist/module/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "contributors": [
    "Anna Krohmer <anna.krohmer@udg.de>",
    "Marcel Bertram <marcel.bertram@porsche.de>",
    "Oliver Hook <oliver.hook@mhp.com>",
    "Stephan Schröter <stephan.schroeter@mhp.com>",
    "Olivia Pala <olivia.pala@udg.de>",
    "Matthias Hägele <matthias.haegele@mhp.com>",
    "Melanie Kolar <melanie.kolar@udg.de>",
    "Bernd Hacker <bernd.hacker@mhp.com>",
    "Anton Schmidt <anton.schmidt@mhp.com>"
  ],
  "license": "SEE LICENSE IN https://designsystem.porsche.com",
  "description": "Porsche Icons",
  "keywords": [
    "porsche",
    "icons"
  ],
  "homepage": "https://icons.porsche.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/porscheui/porsche-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/porscheui/porsche-ui-contribution/issues"
  },
  "scripts": {
    "build": "yarn build:icons && yarn build:lib && yarn build:database",
    "build:icons": "ts-node --project tsconfig.json scripts/build.ts",
    "build:lib": "yarn build:lib:module && yarn build:lib:umd",
    "build:lib:module": "tsc --build tsconfig.lib.module.json",
    "build:lib:umd": "tsc --build tsconfig.lib.umd.json",
    "build:database": "ts-node --project tsconfig.json scripts/database.ts",
    "pushToFrontify": "ts-node --project tsconfig.json scripts/pushToFrontify.ts",
    "clean": "yarn clean:node_modules && yarn clean:build",
    "clean:node_modules": "rm -rf ./node_modules && rm -rf ./yarn-error.log",
    "clean:build": "rm -rf ./index.ts && rm -rf ./dist"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.1",
    "@types/svgo": "^2.6.3",
    "change-case": "^4.1.2",
    "globby": "^11.0.4",
    "js-yaml": "^4.0.0",
    "svgo": "^2.8.0",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.4"
  },
  "files": [
    "dist/"
  ]
}
