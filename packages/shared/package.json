{
  "name": "shared",
  "version": "0.0.0",
  "private": true,
  "scope": "@porsche-design-system",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "license": "UNLICENSED",
  "homepage": "https://designsystem.porsche.com",
  "scripts": {
    "postinstall": "rm -rf ../../node_modules/@types/puppeteer",
    "build": "yarn clean:build && yarn build:generateTagNames && yarn build:generateChunkNames && yarn build:generatePartialNames && yarn build:generateCodeSamples && yarn build:bundle",
    "build:generateTagNames": "ts-node scripts/generateTagNames.ts",
    "build:generateCodeSamples": "ts-node scripts/generateCodeSamples.ts",
    "build:generateChunkNames": "ts-node scripts/generateTagNamesWithChunk.ts",
    "build:generatePartialNames": "ts-node scripts/generatePartialNames.ts",
    "build:bundle": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "clearGitHubActionLogs": "ts-node scripts/clearGitHubActionLogs.ts",
    "test": "yarn test:unit",
    "test:unit": "jest --colors -c tests/unit/config/jest.config.js",
    "clean": "yarn clean:node_modules && yarn clean:build",
    "clean:node_modules": "rm -rf ./node_modules && rm -rf ./yarn-error.log",
    "clean:build": "rm -rf ./src/lib && rm -rf ./dist"
  },
  "dependencies": {
    "@alex_neo/jest-expect-message": "^1.0.5",
    "@porsche-design-system/component-meta": "0.0.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.2",
    "@rollup/plugin-replace": "^5.0.3",
    "@rollup/plugin-typescript": "^11.1.4",
    "@signed/jest-alphabetical-sequencer": "^0.1.1",
    "@types/jest": "^29.5.5",
    "@types/jest-environment-puppeteer": "^5.0.4",
    "@types/jest-expect-message": "^1.1.0",
    "@types/node": "^20.8.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^9.0.1",
    "jss": "^10.10.0",
    "jss-plugin-sort-css-media-queries": "^1.0.1-beta.0",
    "jss-preset-default": "^10.10.0",
    "pixelmatch": "^5.3.0",
    "puppeteer": "~21.3.6",
    "rollup": "^3.29.4",
    "rollup-plugin-bin": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sharp": "^0.32.6",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@octokit/core": "^5.0.1",
    "@types/madge": "^5.0.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/sharp": "^0.32.0",
    "change-case": "^5.0.2",
    "globby": "^11.0.4",
    "madge": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
