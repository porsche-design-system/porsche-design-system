{
  "name": "@porsche-design-system/assets",
  "version": "1.1.0",
  "scope": "@porsche-design-system",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "contributors": [
    "Stephan Schröter <stephan.schroeter@mhp.com>",
    "Oliver Hook <oliver.hook@mhp.com>",
    "Marcel Bertram <marcel.bertram@porsche.de>",
    "Matthias Hägele <matthias.haegele@mhp.com>",
    "Olivia Pala <olivia.pala@udg.de>",
    "Anna Krohmer <anna.krohmer@udg.de>",
    "Melanie Kolar <melanie.kolar@udg.de>",
    "Bernd Hacker <bernd.hacker@mhp.com"
  ],
  "license": "SEE LICENSE IN https://designsystem.porsche.com",
  "description": "Porsche Design System Assets",
  "keywords": [
    "porsche",
    "assets"
  ],
  "homepage": "https://designsystem.porsche.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/porscheui/porsche-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/porscheui/porsche-ui-contribution/issues"
  },
  "scripts": {
    "start": "yarn build && node ./bin/serve-cdn.js",
    "build": "yarn clean:build && yarn build:bin && yarn build:manifest && yarn build:contents",
    "build:bin": "tsc --build './tsconfig.cdn.json' && chmod +x bin/serve-cdn.js",
    "build:contents": "ts-node --project './tsconfig.cdn.json' './scripts/build.ts'",
    "build:manifest": "rollup -c rollup.config.js",
    "clean": "yarn clean:node_modules && yarn clean:build",
    "clean:node_modules": "rm -rf ./node_modules && rm -rf ./yarn-error.log",
    "clean:build": "rm -rf ./dist && rm -rf ./cdn && rm -rf ./bin"
  },
  "devDependencies": {
    "@porsche-design-system/components": "0.0.0",
    "@porsche-design-system/fonts": "0.0.0",
    "@porsche-design-system/icons": "0.0.0",
    "@porsche-design-system/marque": "0.0.0",
    "@porsche-design-system/metaicons": "0.0.0",
    "@porsche-design-system/utilities": "1.0.6",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/http-server": "^0.10.0",
    "http-server": "^0.12.3",
    "rollup": "^2.23.1",
    "rollup-plugin-terser": "^7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "files": [
    "dist/"
  ],
  "bin": {
    "serve-cdn": "./bin/serve-cdn.js"
  }
}
