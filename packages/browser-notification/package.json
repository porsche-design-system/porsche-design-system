{
  "name": "@porsche-design-system/browser-notification",
  "version": "0.0.0",
  "description": "Stand-alone self invoking JS snippet to show a notification in case of unsupported browser features are detected or\nbrowser cookies are disabled.",
  "keywords": [
    "porsche",
    "browser",
    "notification",
    "feature",
    "detection",
    "cookie",
    "support",
    "ie11"
  ],
  "scope": "@porsche-design-system",
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "license": "MIT",
  "homepage": "https://designsystem.porsche.com",
  "private": true,
  "scripts": {
    "start": "yarn build && http-server ./www -p 4201",
    "build": "yarn build:generate-notification && yarn build:tmp && yarn build:cdn+dist && yarn build:www",
    "build:generate-notification": "ts-node './scripts/generate-notification.ts'",
    "build:tmp": "rollup -c rollup.config.js",
    "build:cdn+dist": "ts-node './scripts/generate-cdn-dist.ts'",
    "build:www": "rm -rf ./www && mkdir -p ./www && cp -r ./public/. ./www && cp -r ./tmp/. ./www/.",
    "clean": "yarn clean:node_modules && yarn clean:build",
    "clean:node_modules": "rm -rf ./node_modules && rm -rf ./yarn-error.log",
    "clean:build": " rm -rf ./cdn && rm -rf ./dist && rm -rf ./tmp && rm -rf ./www"
  },
  "devDependencies": {
    "@porsche-design-system/prettier-config": "^1.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/html-minifier": "^4.0.2",
    "change-case": "^4.1.2",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.74.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
