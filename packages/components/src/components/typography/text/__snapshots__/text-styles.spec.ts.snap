  font-weight: 700 !important;
}
p-text em, p-text i {
  font-style: normal !important;
}
p-text a::-moz-focus-inner, p-text button::-moz-focus-inner {
  border: 0 !important;
}
p-text a:focus, p-text button:focus {
  outline-color: currentColor !important;
}
p-text a:focus:not(:focus-visible), p-text button:focus:not(:focus-visible) {
  outline-color: transparent !important;
}
@media(hover:hover) {
  p-text[data-theme="dark"] a:hover, p-text[data-theme="dark"] button:hover {
    color: #ff0223 !important;
  }
  p-text a, p-text button {
    transition: color var(--p-transition-duration, .24s) ease !important;
  }
  p-text a:hover, p-text button:hover {
    color: #d5001c !important;
  }
}
"
`;

