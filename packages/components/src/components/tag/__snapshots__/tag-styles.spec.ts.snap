::slotted(a::-moz-focus-inner) {
  border: 0 !important;
}
::slotted(a:focus) {
  outline: 0 !important;
}
::slotted(a:focus-visible) {
  outline: 0 !important;
}
::slotted(a:focus-visible)::before {
  outline: 2px solid #1A44EA !important;
  outline-offset: 2px !important;
}
::slotted(button::-moz-focus-inner) {
  border: 0 !important;
}
::slotted(button:focus) {
  outline: 0 !important;
}
::slotted(button:focus-visible) {
  outline: 0 !important;
}
::slotted(button:focus-visible)::before {
  outline: 2px solid #1A44EA !important;
  outline-offset: 2px !important;
}
::slotted(br) {
  display: none !important;
}
@media(hover:hover) {
  span:hover {
    background: #535457;
  }
}

.icon {
  margin-inline-start: -2px;
  filter: invert(1);
}"
`;
