slot:not([name]) {
  grid-column: 2/3;
  z-index: 0;
}
slot[name=header] {
  grid-column: 1/-1;
  z-index: 1;
  position: sticky;
  top: 0;
  margin-block: calc(-1 * clamp(16px, 1.25vw + 12px, 36px)) calc(-1 * 16px);
  padding: clamp(16px, 1.25vw + 12px, 36px) clamp(32px, 2.75vw + 23px, 76px) 16px;
  background: linear-gradient(180deg,var(--_a) 0%,var(--_a) 80%,transparent 100%);
  border-start-start-radius: var(--_p-legacy-radius-large, var(--p-radius-xl));
}
slot[name=footer] {
  grid-column: 1/-1;
  z-index: 2;
  position: sticky;
  bottom: -.1px;
  margin-block: calc(-1 * calc(calc(clamp(8px, 0.5vw + 6px, 16px) + clamp(16px, 1.25vw + 12px, 36px)) - var(--_p-legacy-radius-large, var(--p-radius-xl))));
  padding: calc(calc(clamp(8px, 0.5vw + 6px, 16px) + clamp(16px, 1.25vw + 12px, 36px)) - var(--_p-legacy-radius-large, var(--p-radius-xl))) clamp(32px, 2.75vw + 23px, 76px);
  background: linear-gradient(0deg,var(--_a) 0%,var(--_a) 20%,transparent 80%);
}
slot[name=sub-footer] {
  grid-column: 1/-1;
  z-index: 3;
  padding-inline: clamp(32px, 2.75vw + 23px, 76px);
