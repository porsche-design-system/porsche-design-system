  border-end-start-radius: var(--_p-legacy-radius-large, var(--p-radius-xl));
  grid-template-rows: auto 1fr auto;
}
.dismiss {
  grid-area: 1/3;
  z-index: 5;
  position: sticky;
  top: clamp(8px, 0.5vw + 6px, 16px);
  margin-top: calc(-1 * clamp(16px, 1.25vw + 12px, 36px) + clamp(8px, 0.5vw + 6px, 16px));
  margin-inline-end: clamp(8px, 0.5vw + 6px, 16px);
  place-self: flex-start flex-end;
}
.dismiss::after {
  content: "";
