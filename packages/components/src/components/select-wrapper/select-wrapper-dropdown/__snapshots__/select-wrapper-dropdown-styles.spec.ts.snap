exports[`getListStyles() should return correct css for direction: down and theme: dark 1`] = `
"ul {
  display: flex;
  flex-direction: column;
  gap: 8px;
  position: var(--p-internal-dropdown-position);
  padding: 6px;
  margin: 0;
  background: #0E0E12;
  font: normal normal 400 1rem/calc(6px + 2.125ex) 'Porsche Next','Arial Narrow',Arial,'Heiti SC',SimHei,sans-serif;
  overflow-wrap: break-word;
  hyphens: auto;
  z-index: 10;
  left: 0;
  right: 0;
  top: calc(100% - 2px);
  box-sizing: border-box;
  max-height: 416px;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  scroll-behavior: smooth;
  border: 2px solid #FBFCFF;
  border-top: 1px solid #88898C !important;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  scrollbar-width: thin;
  scrollbar-color: auto;
  transition: border-color var(--p-transition-duration, 0.25s) cubic-bezier(0.25,0.1,0.25,1);
  transform: translate3d(0,0,0);
}
.option {
  display: flex;
  justify-content: space-between;
  gap: 12px;
  padding: 8px 12px;
  flex: 1 0 calc(calc(6px + 2.125ex) + 8px * 2);
  color: #AFB0B3;
  cursor: pointer;
  text-align: start;
  word-break: break-word;
  box-sizing: border-box;
  border-radius: 4px;
  transition: background-color var(--p-transition-duration, 0.25s) cubic-bezier(0.25,0.1,0.25,1), color var(--p-transition-duration, 0.25s) cubic-bezier(0.25,0.1,0.25,1);
}
.option[role=status] {
  cursor: not-allowed;
}
.option__sr {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0,0,0,0);
  white-space: nowrap;
}
.option--selected {
  cursor: default;
  pointer-events: none;
  background: #212225;
}
.option--highlighted {
  background: #404044;
}
.option--highlighted, .option--selected {
  color: #FBFCFF;
}
.option--disabled {
  cursor: not-allowed;
  color: #7E7F82;
}
.option--hidden {
  display: none;
}
.optgroup {
  display: block;
  padding: 3px 14px;
  font-weight: 600;
}
.optgroup:not(:first-child) {
  margin-top: 8px;
}
.optgroup~.option {
  padding-left: 24px;
}
@media(hover:hover) {
  .option:not([aria-disabled]):not(.option--disabled):not([role=status]):hover {
    color: #FBFCFF;
    background: #404044;
  }
}
"
`;

