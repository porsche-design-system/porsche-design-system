  top: 0 !important;
  left: 0 !important;
  right: 0 !important;
  bottom: 0 !important;
  z-index: 99999 !important;
  display: flex !important;
  align-items: center !important;
  justify-content: center !important;
  flex-wrap: wrap !important;
  transition: opacity .6s cubic-bezier(.16,1,.3,1) !important;
  opacity: 1 !important;
  visibility: inherit !important;
  overflow-y: auto;
}
:host::before {
  content: "" !important;
  position: fixed !important;
  top: 0 !important;
  left: 0 !important;
  right: 0 !important;
  bottom: 0 !important;
  background: #0e1418e6 !important;
}
::slotted(.stretch-to-full-modal-width) {
  width: calc(100% + 4rem) !important;
  margin: 0 -2rem !important;
}
::slotted(.stretch-to-full-modal-width:last-child) {
  margin-bottom: -2rem !important;
}
h1 {
  font: normal normal 400 1.5rem/1.5 'Porsche Next','Arial Narrow',Arial,'Heiti SC',SimHei,sans-serif;
  text-size-adjust: none;
  -webkit-text-size-adjust: none;
  overflow-wrap: break-word;
  hyphens: auto;
  font-weight: 600;
  margin: 0;
  color: #000;
}
@media(min-width:1000px) {
  ::slotted(.stretch-to-full-modal-width) {
    width: calc(100% + 5rem) !important;
    margin: 0 -2.5rem !important;
  }
  ::slotted(.stretch-to-full-modal-width:last-child) {
    margin-bottom: -2.5rem !important;
  }
  h1 {
    font: normal normal 400 2.25rem/1.3333333333 'Porsche Next','Arial Narrow',Arial,'Heiti SC',SimHei,sans-serif;
    text-size-adjust: none;
    -webkit-text-size-adjust: none;
    overflow-wrap: break-word;
    hyphens: auto;
    font-weight: 600;
  }
}

@media(min-width:1920px) {
  ::slotted(.stretch-to-full-modal-width) {
    width: calc(100% + 8rem) !important;
    margin: 0 -4rem !important;
  }
  ::slotted(.stretch-to-full-modal-width:last-child) {
    margin-bottom: -4rem !important;
  }
}

.root {
  position: relative;
  box-sizing: border-box;
  transition: transform .6s cubic-bezier(.16,1,.3,1);
  transform: scale3d(1,1,1);
  padding: 2rem;
  background-color: #fff;
  outline: 1px solid transparent;
  outline-offset: 2px;
  min-width: 100%;
  max-width: none;
  min-height: 100%;
  margin: 0;
}
.root::-moz-focus-inner {
  border: 0;
}
.root:focus {
  outline-color: #fff;
}
.root:focus:not(:focus-visible) {
  outline-color: transparent;
}
.header {
  padding: 0 0 1rem;
  margin: 0 2rem 0 0;
}
.close {
  position: absolute;
  top: 0;
  right: 0;
  padding: 0.5rem;
  border: 0.375rem solid #fff;
  background: #fff;
}
@media(min-width:1000px) {
  .root {
    padding: 2.5rem;
  }
  .header {
    padding: 0 0 1.5rem;
  }
}

@media(min-width:1760px) {
  .root {
    margin: 0;
  }
}

@media(min-width:1920px) {
  .root {
    padding: 4rem;
    margin: 0;
  }
  .header {
    padding: 0 0 2rem;
    margin: 0;
  }
}
"
`;

