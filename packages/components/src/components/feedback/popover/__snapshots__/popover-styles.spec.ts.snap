exports[`getSlottedCss() should return correct css 1`] = `
"p-popover a {
  color: inherit !important;
  text-decoration: underline !important;
  outline: 1px solid transparent !important;
  outline-offset: 1px !important;
}
p-popover b, p-popover strong {
  font-weight: 700 !important;
}
p-popover em, p-popover i {
  font-style: normal !important;
}
p-popover a::-moz-focus-inner {
  border: 0 !important;
}
p-popover a:focus {
  outline-color: currentColor !important;
}
p-popover a:focus:not(:focus-visible) {
  outline-color: transparent !important;
}
@media(hover:hover) {
  p-popover a {
    transition: color var(--p-transition-duration, .24s) ease !important;
  }
  p-popover a:hover {
    color: #d5001c !important;
  }
}
"
`;

