  inset: auto max(22px, 10.625vw - 12px) var(--_p-a) !important;
  z-index: 999999 !important;
}
:host([hidden]) {
  display: none !important;
}
:not(:defined,[data-ssr]) {
  visibility: hidden;
}
@keyframes in {
  from {
    opacity: 0;
    transform: translate3d(0,calc(var(--_p-a) + 100%),0);
