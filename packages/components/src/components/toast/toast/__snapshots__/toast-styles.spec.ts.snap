  to {
    opacity: 1;
    transform: translate3d(0,0,0);
  }
}
@keyframes out {
  from {
    opacity: 1;
    transform: translate3d(0,0,0);
  }
  to {
    opacity: 0;
    transform: translate3d(0,calc(var(--_p-a) + 100%),0);
  }
}
@media(min-width:760px) {
    :host {
      --_p-a: var(--p-toast-position-bottom, 64px) !important;
      inset: auto auto var(--_p-a) 64px !important;
      max-width: min(42rem, calc(100vw - 64px * 2)) !important;
    }
}

  .hydrated {
    animation: .4s in cubic-bezier(0,0,.2,1) forwards;
  }
  .close {
    animation: .4s out cubic-bezier(.4,0,.5,1) forwards !important;
  }"
`;

