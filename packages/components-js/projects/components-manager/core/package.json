{
  "name": "@porsche-design-system/web-components-manager",
  "version": "1.0.0-alpha.6",
  "private": true,
  "main": "dist/esm5/index.js",
  "esm5": "dist/esm5/index.js",
  "esm2015": "dist/esm2015/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/types",
    "dist/esm5",
    "dist/esm2015"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:esm2015 && yarn build:esm5",
    "build:esm5": "babel dist/esm2015 --source-maps true --out-dir dist/esm5",
    "build:esm2015": "tsc --build tsconfig.json",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "clean": "yarn clean:node_modules && yarn clean:build",
    "clean:node_modules": "rm -rf ./node_modules && rm -rf ./yarn-error.log",
    "clean:build": "rm -rf ./dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.20",
    "babel-jest": "^26.6.3",
    "core-js-bundle": "^3.8.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
